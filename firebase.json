{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    } ]
  }
}
